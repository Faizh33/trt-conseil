{% extends 'base.html.twig' %}

{% block title %}Créez votre compte{% endblock %}

{% block body %}
{{ form_start(form, {'attr': {'class': 'form', 'style': 'margin-bottom: 16vh;'}}) }}
    <h1 class="form-title">Créez votre compte</h1>
    {% for field in form %}
        {{ form_row(field) }}
    {% endfor %}
    <input type="submit" class="button form-btn" value="Enregistrer" />
{{ form_end(form) }}
{% endblock %}
